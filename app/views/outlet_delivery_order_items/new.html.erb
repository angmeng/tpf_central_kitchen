<h1>New outlet_delivery_order_item</h1>

<% form_for(@outlet_delivery_order_item) do |f| %>
  <% render "/shared/error_messages", :target => @outlet_delivery_order_item %>
  <p>
    <%= f.label :outlet_delivery_order_id %><br />
    <%= f.text_field :outlet_delivery_order_id %>
  </p>
  <p>
    <%= f.label :product_id %><br />
    <%= f.text_field :product_id %>
  </p>
  <p>
    <%= f.label :quantity %><br />
    <%= f.text_field :quantity %>
  </p>
  <p>
    <%= f.label :unit_price %><br />
    <%= f.text_field :unit_price %>
  </p>
  <p>
    <%= f.label :remark %><br />
    <%= f.text_field :remark %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', outlet_delivery_order_items_path %>