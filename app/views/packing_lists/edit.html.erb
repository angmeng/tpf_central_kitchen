<h1>Editing packing_list</h1>

<%= form_for(@packing_list) do |f| %>
   <% render "/shared/error_messages", :target => @packing_list %>
  <p>
    <%= f.label :packing_list_number %><br />
    <%= @packing_list.packing_list_number %>
  </p>
  <p>
    <%= f.label :packing_date %><br />
    <%= f.date_select :packing_date %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description, :size => "60x8" %>
  </p>

  <p>
    <%= f.submit 'Update' %> |
	<%= link_to 'Show', @packing_list %> |
    <%= link_to 'Back', packing_lists_path %>

  </p>
<% end %>

