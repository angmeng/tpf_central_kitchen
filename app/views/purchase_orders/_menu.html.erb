<p>
  <%= link_to image_tag("edit.png", :title => "Edit"), edit_purchase_order_path(@purchase_order) %> |
  <%= link_to(image_tag("print.png", :title => "Print"), preview_purchase_order_path(@purchase_order), {:target => "_blank"}) %> 
  <% unless @purchase_order.settled? %>
   | <%= link_to image_tag("settle.png", :size => "28x28", :title => "Mark this PO as Completed"), complete_purchase_order_path(@purchase_order), :confirm => 'Are you sure you want to mark this PO as Completed ?' %> 
  <% end %>
  
  <% link_to image_tag("delete.png", :title => "Delete"), @purchase_order, :confirm => 'Are you sure?', :method => :delete %> 
  <% link_to image_tag("home.png", :title => "View All"), purchase_orders_path %>  
  <% link_to image_tag("new.png", :title => "New"), new_purchase_order_path %>

   <% if @purchase_order.outlet_purchase_order %>
   |  <%= link_to image_tag("back.png", :title => "Back to Outlet Purchase order"), @purchase_order.outlet_purchase_order %> 
   <% end %>
  
    
</p>