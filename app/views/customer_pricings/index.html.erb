<h1>Listing customer_pricings</h1>

<table class="list">
  <thead>
  <tr>
    <th>Customer</th>
    <th>Product</th>
    <th>Amount</th>
  </tr>
</thead>
   <tbody>
<% @customer_pricings.each do |customer_pricing| %>
 <tr class="<%= cycle("odd", "even") %>">
    <td><%=h customer_pricing.customer_id %></td>
    <td><%=h customer_pricing.product_id %></td>
    <td><%=h customer_pricing.amount %></td>
    <td><%= link_to 'Show', customer_pricing %></td>
    <td><%= link_to 'Edit', edit_customer_pricing_path(customer_pricing) %></td>
    <td><%= link_to 'Destroy', customer_pricing, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</tbody>
</table>

<br />

<%= link_to 'New customer_pricing', new_customer_pricing_path %>