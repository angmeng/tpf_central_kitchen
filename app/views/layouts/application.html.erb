<!DOCTYPE html>
<html>
<head>
  <title><%= h(yield(:title) || "NIT POS") %></title>
  <%#= javascript_include_tag :defaults, "jquery", "jquery_ujs", "jquery-ui", "autocomplete-rails.js" %>
  <%= javascript_include_tag 'application' %>
  <%= stylesheet_link_tag 'application' %>
  <%= include_tiny_mce_if_needed %>
  
    <script>
    // jQuery.noConflict();
   //   // Use jQuery via jQuery(...)
    // jQuery(document).ready(function(){

    // });

   $( document ).ready(function() {

   });

     </script>
    <%= yield(:head) %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if current_user %>
   <div id='header'>
        <%= render :partial => '/layouts/header' %>
       </div>
	   
      <% if is_admin? %>
	        <%= render :partial => '/layouts/admin_menu'  %>
	    <% elsif is_outlet_staff? %>
		    <%= render :partial => '/layouts/outlet_staff_menu'  %>
	    <% end %>
<% end %>
<div style='float:left;margin:5px 0px 0px 15px;width:80%;'>
  <% flash.each do |name, msg| %>
    <%= content_tag :p, msg, :class => "flash_#{name}" %>
  <% end  %>
</div>
<div style="clear:both"></div>

	<table cellpadding='0' cellspacing='0' class='tabbed' height='100%' width='100%'>
       <tr>
        <td class='main' id='main' valign='top'>

            <div class='title' id='title' style="clear: both;"> <%= @page_title || "" %> </div>

         <%= yield %>
       </td>
      </tr>
    </table>

   <%= render :partial => "/layouts/footer"%>  
   <%= yield(:footer) %>

</body>
</html>
