<%= form_for @expense do |f| %>
 <% render "/shared/error_messages", :target => @expense %>  <p>
    <%= f.label :expense_date, (t "expenses.expense_date") %><br />
    <%= f.date_select :expense_date %>
  </p>
  <p>
    <%= f.label :title, (t "expenses.title") %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :description, (t "common.description") %><br />
    <%= f.text_area :description, :size => "60x8" %>
  </p>
  
  
  <p>
    <%= f.label :amount, (t "expenses.amount") %><br />
    <%= f.text_field :amount %>
  </p>
 
  <p><%= f.submit(t "links.submit") %></p>
<% end %>
