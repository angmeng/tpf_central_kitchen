<h1>New outlet_delivery_order</h1>

<%= form_for(@outlet_delivery_order) do |f| %>
  <% render "/shared/error_messages", :target => @outlet_delivery_order %>
  <p>
    <%= f.label :outlet_id %><br />
    <%= f.text_field :outlet_id %>
  </p>
  <p>
    <%= f.label :delivery_order_date %><br />
    <%= f.date_select :delivery_order_date %>
  </p>
  <p>
    <%= f.label :delivery_order_number %><br />
    <%= f.text_field :delivery_order_number %>
  </p>
  <p>
    <%= f.label :remark %><br />
    <%= f.text_area :remark %>
  </p>
  <p>
    <%= f.label :void %><br />
    <%= f.check_box :void %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', outlet_delivery_orders_path %>